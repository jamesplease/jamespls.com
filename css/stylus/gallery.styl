.illustration-list
  display flex
  flex-flow row wrap

  li
    width 200px
    height 200px
    border-radius 2px
    border 1px solid #ddd
    padding 4px
    
  figure
    background-color #eee
    padding 0
    margin 0
    height 100%
    width 100%
    
    img
      width 100%
      height 100%